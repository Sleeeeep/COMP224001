package buyer;
import java.io.IOException;
import java.util.*;


public class Buyer {
	private String name;
	private int age;
	private String Phone;
	
	public Buyer(String name, age, Phone)
	{
		
	}

	public String getName() {
		return name;
	}
	

	public void setName(String name) {
		this.name = name;
	}


	public int getAge() {
		return age;
	}


	public void setAge(int age) {
		this.age = age;
	}


	public String getPhone() {
		return Phone;
	}


	public void setPhone(String phone) {
		Phone = phone;
	}



}
